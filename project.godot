; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "NearConnection",
"language": "GDScript",
"path": "res://addons/godot-near-sdk/NearConnection.gd"
}, {
"base": "Reference",
"class": "WalletConnection",
"language": "GDScript",
"path": "res://addons/godot-near-sdk/WalletConnection.gd"
} ]
_global_script_class_icons={
"NearConnection": "",
"WalletConnection": ""
}

[application]

config/name="Gamedevjs2022"
run/main_scene="res://Gameplay.tscn"
config/icon="res://icon.png"

[autoload]

Near="*res://addons/godot-near-sdk/Near.tscn"
CryptoProxy="*res://addons/godot-near-sdk/utils/CryptoProxy.gd"

[display]

window/size/width=256
window/size/height=144
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
